{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "clipboard:allow-write-image-base64",
    "clipboard:allow-write-text",
    "clipboard:allow-read-text",
    "fs:allow-write-file",
    "fs:allow-read-file",
    "fs:allow-read-dir",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "dialog:allow-save",
    "dialog:allow-open"
  ]
}
